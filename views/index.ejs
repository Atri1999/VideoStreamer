<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Video Stream</title>
    <link rel="stylesheet" href="index.css">
    <script src="index.js"></script>
</head>
<body>
    <header id="header">Video Streamer</header>
    <div class="container">
        <% listFiles.forEach(element => { %>
            <div>
                <a href="/stream/<%= element %> ">
                    <img src="./thumbnails/<%= element %>.jpg" width="100%" height="100%">
                </a>
                <div class="displayName"><%= element.split('.')[0] %> </div>
            </div>
        <% }) %>
    </div>
</body>
</html>